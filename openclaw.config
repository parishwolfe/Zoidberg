{
  "model": {
    "provider": "ollama",
    "baseUrl": "${OLLAMA_BASE_URL}",
    "model": "llama3:13b"
  },

  "channels": {
    "bluebubbles": {
      "enabled": true,
      "url": "${BLUEBUBBLES_URL}",
      "apiKey": "${BLUEBUBBLES_API_KEY}",
      "readOnly": true
    }
  },

  "integrations": {
    "gmail": {
      "imap": {
        "host": "imap.gmail.com",
        "port": 993,
        "secure": true,
        "user": "${GMAIL_IMAP_USER}",
        "pass": "${GMAIL_IMAP_PASS}"
      },
      "readOnly": true
    },

    "googleCalendar": {
      "clientId": "${GOOGLE_CALENDAR_CLIENT_ID}",
      "clientSecret": "${GOOGLE_CALENDAR_CLIENT_SECRET}",
      "scopes": [
        "https://www.googleapis.com/auth/calendar.readonly"
      ]
    },

    "googleMaps": {
      "apiKey": "${GOOGLE_MAPS_API_KEY}"
    }
  }
}